{UTF-8}
Recovery rate=Identified Infected * Immunity
	~	1/Day
	~	  Smoothed recovery rate with reduced maximum
	|

Testing Start Time=
    30
	~	Day
	~	  Time delay before testing program begins
	|

Testing=IF THEN ELSE(Time >= Testing Start Time,
    SMOOTH3(MIN(Infected, Testing accuracy*Testing Rate*Infected/MAX(1, Total population\
		)), 2),
    0)
	~	People/Day
	~	  Smoothed daily testing rate, begins after specified delay
	|

vaccination available=(0.04)*Medication research / Vaccine cost
	~	People/Day
	~	  Daily vaccine supply with proper unit conversion from funding to \
		physical         vaccines
	|

Testing Cost=1000
	~	 [0,10000]
	~		|

Vaccine cost=1000
	~	 [0,10000]
	~		|

Testing Rate=MIN(1000, Government Investment*Testing fund percentage/ Testing Cost)
	~	USD
	~	  Bounded testing capacity
	|

Infection=MIN(Healthy non vaccinated people,
    MIN(1000, Infected*Healthy non vaccinated people*Infection rate/MAX(100, Total population\
		)))
	~	People/Day
	~	  Daily new infections with upper bound
	|

Max Vaccine=10
	~	 [0,1000]
	~		|

Infection rate=(Virus mutation * (1 - Social distancing)) / Infection period
	~	1/Day
	~	  Per capita rate of new infections among susceptible population, reduced \
		by social distancing
	|

Vaccination Start Time=
    60
	~	Day
	~	  Time delay before vaccination program begins
	|

Vaccination=IF THEN ELSE(Time >= Vaccination Start Time,
    MIN(Healthy non vaccinated people,
        MIN(Healthy non vaccinated people*Vaccination rate , Max Vaccine)),
    0)
	~	People/Day
	~	  Daily number of people receiving primary or booster vaccinations, with         \
		safety limits. Starts after specified delay.
	|

Treatment Delay Time=7
	~	Day [1,100]
	~	  Average time required for treatment before patient can be discharged
	|

Rehabilitation=SMOOTH3(
    MIN(Identified Infected, Identified Infected*Recovery rate),
    Treatment Delay Time)
	~	People/Day
	~	  Smoothed recovery rate with capacity limits
	|

Social distancing=MIN(0.9, Civic Factor*Social Awareness Program Fund/MAX(1000, Total population\
		))
	~	Dmnl [0,1]
	~	  Degree of contact reduction from baseline
	|

Mortality=Identified Infected * (Mortality Rate)
	~	People/Day
	~	  Daily deaths from the disease
	|

Breakthrough rate=
    0.15/30
	~	1/Day
	~	  Daily rate at which vaccinated people become infected despite immunity
	|

Breakthrough=
    Breakthrough rate*Healthy vaccinated people
	~	People/Day
	~	  Daily number of vaccinated people who become infected
	|

Campaign Fund Percentage=
    0.2
	~	Dmnl [0,1]
	~	  Fraction of government funds allocated to awareness campaigns
	|

Civic Factor=
    0.7
	~	Dmnl [0,1]
	~	  Population compliance factor with public health measures
	|

Death= INTEG (
    Mortality,
        0)
	~	People
	~	  Cumulative deaths from the disease
	|

Demand of vaccine=MIN(Healthy non vaccinated people * 0.05, Healthy non vaccinated people\
		         * Civic Factor * Social Awareness Program Fund)
	~	People/Day
	~	  Daily population demand for vaccines with upper safety limit
	|

Discharged=SMOOTH3(Recovered * (0.2/7), 2)
	~	People/Day
	~	  Smoothed discharge rate with reduced flow
	|

Government Investment=1e+08
	~	USD [0,1e+08]
	~	  Total government funding for pandemic response
	|

Healthy non vaccinated people= INTEG (
    -MIN(Healthy non vaccinated people, Infection + Vaccination),
    100000)
	~	People
	~	  Population that has not been infected or vaccinated, with protection \
		against negatives
	|

Healthy vaccinated people= INTEG (
    MIN(Discharged + Vaccination, MAX(0, Total population)) - MIN(Breakthrough, Healthy vaccinated people\
		),
    0)
	~	People
	~	  Population that has been vaccinated and not currently infected
	|

Identified Infected= INTEG (
    Testing - MIN(Identified Infected, Mortality + Rehabilitation),
    0)
	~	People
	~	  Currently under treatment population
	|

Immunity=
    0.7
	~	Dmnl [0,1]
	~	  Effectiveness factor of immunity in preventing severe disease
	|

Infected= INTEG (
    MIN(Breakthrough + Infection, MAX(0, Total population)) - MIN(Testing, Infected),
    1000)
	~	People
	~	  Currently infected but not yet identified population
	|

Total population=
    Healthy non vaccinated people + Healthy vaccinated people + Infected + Identified Infected\
		         + Recovered
	~	People
	~	  Dynamic calculation of total living population
	|

Medication Fund Percentage=
    0.2
	~	Dmnl [0,1]
	~	  Fraction of government funds allocated to medication development
	|

Medication research=
    MAX(0, Government Investment*Medication Fund Percentage)
	~	USD
	~	  Research funding for medication and vaccine development
	|

Mortality Rate=0.05
	~	
	~		|

Recovered= INTEG (
    Rehabilitation - MIN(Recovered, Discharged),
    0)
	~	People
	~	  Recovered population awaiting discharge
	|

Social Awareness Program Fund=
    Campaign Fund Percentage*Government Investment
	~	USD
	~	  Funding allocated to public health awareness campaigns
	|

Testing accuracy=0.4
	~	Dmnl [0,1]
	~	  Proportion of true positives correctly identified by tests
	|

Testing fund percentage=
    0.3
	~	Dmnl [0,1]
	~	  Fraction of government funds allocated to testing infrastructure
	|

Virus mutation=0.1
	~	Dmnl [0,1]
	~	  Reduced transmissibility factor
	|

Vaccination rate=
    MIN(0.05, MIN(Demand of vaccine/MAX(10, Healthy non vaccinated people), 
        vaccination available/MAX(100, Healthy non vaccinated people)))
	~	1/Day
	~	  Fraction of eligible population receiving vaccination daily
	|

Infection period=1
	~	1/Day [0,2]
	~	  Base infection rate per contact before adjustments
	|

Economic Impact= INTEG (
    Daily economic loss,
        0)
	~	USD
	~	  Cumulative economic impact of the pandemic
	|

Daily economic loss=
    (Infected + Identified Infected) * 50 + Social distancing * Total population * 0.1
	~	USD/Day
	~	  Daily economic cost due to illness and restrictions
	|

R0=
    Infection rate * Infection duration
	~	Dmnl
	~	  Basic reproduction number; average number of secondary infections per \
		case
	|

Infection duration=
    10
	~	Day
	~	  Average duration of infectious period
	|

********************************************************
	.Control
********************************************************~
		        Simulation Control Parameters
	|

FINAL TIME  = 1095
	~	Day
	~	Simulation period to three years
	|

INITIAL TIME  = 0
	~	Day
	~	  The initial time for the simulation.
	|

SAVEPER  = 
        TIME STEP
	~	Day [0,?]
	~	  The frequency with which output is stored.
	|

TIME STEP  = 1
	~	Day [0,?]
	~	  Daily time steps for more granular analysis
	|

